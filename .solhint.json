{
  "extends": "solhint:recommended",
  "rules": {
    "compiler-version": "off",
    "func-visibility": "off",
    "compiler-fixed": false,
    "quotes": "off",
    "indent": "off",
    "func-natspec": "off",
    "no-empty-blocks": "off",
    "no-inline-assembly": "off",
    "interface-starts-with-i": "off",
    "use-natspec": "off",
    "gas-indexed-events": "off",
    "named-parameters-mapping": "off",
    "state-visibility": "off"
  }
}
